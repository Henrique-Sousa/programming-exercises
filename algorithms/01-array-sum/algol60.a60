begin
    integer procedure sumarray(n, a);
        value n;
        integer n;
        integer array a;
    begin
        integer sum, i;
        sum := 0;
        for i:=0 step 1 until n-1 do
            sum := sum + a[i];
        sumarray := sum
    end;

    integer size;
    integer array arr[0:5];

    size := 6;

    arr[0] := 1;
    arr[1] := 2;
    arr[2] := 3;
    arr[3] := 4;
    arr[4] := 10;
    arr[5] := 11;

    outinteger(1, sumarray(size, arr));
    outstring(1, "\n");
end
